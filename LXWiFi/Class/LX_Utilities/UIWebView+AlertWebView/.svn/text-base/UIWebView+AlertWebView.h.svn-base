#import <UIKit/UIKit.h>

@interface UIWebView (AlertWebView)
/**
*  捕捉webView 弹出的提示框时间
*
*  @param sender  加载的WebView
*  @param message 提示信息
*  @param frame   nil
*/
- (void)webView:(UIWebView *)sender runJavaScriptAlertPanelWithMessage:(NSString *)message initiatedByFrame:( id)frame;
@end
